---
{% extends 'layouts/default' %}

{% block content %}

<!-- Slider ============================================= -->
  
  {% block slider, short_name: true, priority: 1 %}

    <section id="slider" class="swiper_wrapper full-screen clearfix">

      <div class="swiper-container swiper-parent">
        <div class="swiper-wrapper">


          <div class="swiper-slide" style="background-image: url(
          {% editable_file 'image1' %}
            /samples/slider/1.jpg
          {% endeditable_file %});">
          
            <div class="container clearfix">
              <div class="slider-caption slider-caption-right" style="max-width: 700px;">

                
                  <h2 data-caption-animate="flipInX">
                    {% editable_text "header1" %}
                      Team of Experts<span>.</span>
                    {% endeditable_text %}    
                  </h2>
                

                <p data-caption-animate="flipInX" data-caption-delay="500">

                  {% editable_text "sub header1" %}
                    Our Team of Doctors are specialized in Various Disciplines to make sure you get the Best Treatment.
                  {% endeditable_text %}

                </p>
              </div>
            </div>
          </div>

          <div class="swiper-slide" style="background-image: url(
          {% editable_file 'image2' %}
            /samples/slider/2.jpg
          {% endeditable_file %});">

            <div class="container clearfix">
              <div class="slider-caption">

                
                  <h2 data-caption-animate="zoomIn">

                    {% editable_text "header2" %}
                      Heart<span>Beat</span>.
                    {% endeditable_text %}

                  </h2>
                

                <p data-caption-animate="zoomIn" data-caption-delay="500">
                  {% editable_text "sub header2" %}
                    Care for your Loved Ones from the Experts in the Medical &amp; Hospitality Industry.
                  {% endeditable_text %}
                </p>
              </div>
            </div>

          </div>

        </div>
      </div>
      


      

    </section>

  {% endblock %}


<!-- #slider end -->


<!-- Info ============================================= -->

  {% block info, short_name: true, priority: 1 %}

    <section id="content">
      <div class="content-wrap">
        
        <div class="container clearfix info_entry_table">

        {% editable_model info_entry, hint: 'some text' %}

            {% for entry in contents.info_entry %}

              <div class="{{entry.style_class}}">
                <div class="feature-box fbox-plain">
                  
                  <div class="fbox-icon" data-animate="bounceIn">
                    <a href="#"><img src="{{entry.image.url}}"></a>
                  </div>

                    <h3>{{entry.header}}</h3>
                 
                    <p>{{entry.body}}</p>
                  
                </div>
              </div>

            {% endfor %}

        {% endeditable_model %}

        </div>

  {% endblock %}

<!-- #Info end -->

<!-- Contact Form ÃŸ============================================= -->

  {% block contact, short_name: true, priority: 1 %}

        <div class="section nopadding common-height dark topmargin-sm">
          <div class="col-md-5" style="background: url(
          {% editable_file 'image1' %}
            /samples/slider/1.jpg
          {% endeditable_file %}) center center no-repeat; background-size: cover;">
            
        </div>

          <div id="booking-appointment-form" class="col-md-7 nopadding">
            <div class="bgcolor col-padding">

              {% model_form 'customer_messages', success: '/', error: '/' %}

                {% if customer_message.errors %}
                  <p>The following errors occured:</p>
                  <ul>
                  {% for error in customer_message.errors %}
                    <li>{{error[0] | capitalize}} -  {{error[1]}}</li>
                  {% endfor %}
                  </ul>
                {% endif %}

              <h2>Book an Appointment.</h2>
              <div id="medical-form-result" data-notify-type="success" data-notify-msg="<i class=icon-ok-sign></i> Message Sent Successfully!"></div>
             
                <div class="col_two_third">
                  <label for="template-medical-name">Name:</label>
                  <input type="text" id="template-medical-name" name="content[name]" value="{{customer_message.name}}" class="form-control not-dark required">
                </div>
                <div class="col_one_third col_last">
                  <label for="template-medical-phone">Phone:</label>
                  <input type="text" id="template-medical-phone" name="content[phone]" value="{{customer_message.phone}}" class="form-control not-dark required">
                </div>
                <div class="clear"></div>
                <div class="col_two_third">
                  <label for="template-medical-email">Email Address:</label>
                  <input type="email" id="template-medical-email" name="content[email]" value="{{customer_message.email}}" class="form-control not-dark required">
                </div>
                <div class="col_one_third col_last">
                  <label for="template-medical-dob">Date of Birth:</label>
                  <input type="text" id="template-medical-dob" name="content[dob]" value="{{customer_message.dob}}" class="form-control not-dark required" placeholder="DD/MM/YYYY">
                </div>
                <div class="clear"></div>
                <div class="col_two_fifth nobottommargin">
                  <div class="col_full">
                    <label for="template-medical-appoint-date">Appointment Date:</label>
                    <input type="text" id="template-medical-appoint-date" name="content[app_date]" value="{{customer_message.app_date}}" class="form-control not-dark required" value="" placeholder="DD/MM/YYYY">
                  </div>
                  
                </div>
                <div class="col_three_fifth nobottommargin col_last">
                  <label for="template-medical-message">Message:</label>
                  <textarea id="template-medical-message" name="content[message]" class="form-control not-dark required" cols="30" rows="5">{{customer_message.message}}</textarea>
                </div>
                <div class="clear"></div>
                
                <div class="col_full topmargin-sm nobottommargin">
                  
                  <button class="button button-rounded button-white button-light nomargin" type="submit" value="submit">Confirm Booking</button>

                  {% endmodel_form %}

                </div>

                <div class="clear"></div>

            </div>
          </div>
        </div>
  {% endblock %}

<!-- #Contact end -->


<!-- Testimonials ============================================= -->

  
        <div class="container clearfix">

          {% block treatments, short_name: true, priority: 1 %}

            <div class="col_three_fifth">
              <div class="accordion accordion-lg clearfix">

                {% editable_model treatments, hint: 'some text' %}

                  {% for treat in contents.treatments %}

                    <div class="acctitle"><i class="acc-closed icon-medical-i-kidney color"></i><i class="acc-open icon-medical-kidney color"></i>{{treat.name}}</div>
                    <div class="acc_content clearfix">{{treat.body}}</div>

                  {% endfor %}

                {% endeditable_model %}

              </div>
            </div>

          {% endblock %}

          {% block testimonials, short_name: true, priority: 1 %}

            <div class="col_two_fifth col_last">

              {% capture test_header %}
                 {% editable_text "test_header" %}
                    Patient Testimonials<span>.</span>
                 {% endeditable_text %}
              {% endcapture %}

              <h4>
                {{ test_header }}              
              </h4>

              <ul class="testimonials-grid grid-1 clearfix">

                {% editable_model testimonials, hint: 'some text' %}

                  {% for test in contents.testimonials %}

                    <li class="noleftpadding notoppadding">
                      <div class="testimonial">

                        <div class="testi-image">
                          <a href="#"><img src="{{test.image.url}}" alt="Customer Testimonails"></a>
                        </div>

                        <div class="testi-content">
                          <p>{{test.body}}</p>
                          <div class="testi-meta">
                            {{test.name}}
                            <span>{{test.company}}</span>
                          </div>
                        </div>

                      </div>
                    </li>

                  {% endfor %}
                {% endeditable_model %}

              </ul>

              <div class="clear"></div>
              <a href="#" class="button button-mini button-rounded norightmargin fright">More Patient Feedbacks...</a>
              <div class="clear"></div>

            </div>
          {% endblock %}
        </div>
        
<!-- #Testimonials end -->

        <div class="section notopmargin">
          <div class="container clearfix">

            <div class="row">
              <div class="col-md-3 col-sm-6">
                <div class="feature-box fbox-outline fbox-dark fbox-effect clearfix">
                  <div class="fbox-icon">
                    <a href="#"><i class="icon-stack i-alt"></i></a>
                  </div>
                  <div class="counter counter-small"><span data-from="100" data-to="23331" data-refresh-interval="200" data-speed="2500"></span>+</div>
                  <h5 class="nomargin color">Clients Served</h5>
                  <div class="visible-xs visible-sm bottommargin"></div>
                </div>
              </div>

              <div class="col-md-3 col-sm-6">
                <div class="feature-box fbox-outline fbox-dark fbox-effect clearfix">
                  <div class="fbox-icon">
                    <a href="#"><i class="icon-tint i-alt"></i></a>
                  </div>
                  <div class="counter counter-small"><span data-from="100" data-to="56841" data-refresh-interval="250" data-speed="2000"></span>+</div>
                  <h5 class="nomargin color">X-Rays Done</h5>
                  <div class="visible-xs visible-sm bottommargin"></div>
                </div>
              </div>

              <div class="col-md-3 col-sm-6">
                <div class="feature-box fbox-outline fbox-dark fbox-effect clearfix">
                  <div class="fbox-icon">
                    <a href="#"><i class="icon-tint i-alt"></i></a>
                  </div>
                  <div class="counter counter-small"><span data-from="100" data-to="332" data-refresh-interval="50" data-speed="3000"></span>+</div>
                  <h5 class="nomargin color">Worldwide Staff</h5>
                  <div class="visible-xs bottommargin"></div>
                </div>
              </div>

              <div class="col-md-3 col-sm-6">
                <div class="feature-box fbox-outline fbox-dark fbox-effect clearfix">
                  <div class="fbox-icon">
                    <a href="#"><i class="icon-text-width i-alt"></i></a>
                  </div>
                  <div class="counter counter-small"><span data-from="100" data-to="2213" data-refresh-interval="110" data-speed="3500"></span>+</div>
                  <h5 class="nomargin color">Lives Saved</h5>
                </div>
              </div>
            </div>

          </div>
        </div>

<!-- Team ============================================= -->
      {% block team_members, short_name: true, priority: 1 %}

        <div class="container clearfix">
          
          <div class="heading-block center nobottomborder">
            <h3>Meet our Team of Specialists<span>.</span></h3>
            <span>We make sure that your Life are in Good Hands.</span>
          </div>

          <div id="oc-team" class="owl-carousel team-carousel">

            {% editable_model team_members, hint: 'some text' %}

              {% for member in contents.team_members %}
            
                <div class="team">
                  <div class="team-image">
                    <img src="{{ member.image.url }}" alt="{{member.name}}">
                  </div>
                  <div class="team-desc">
                    <div class="team-title"><h4>{{member.name}}</h4><span>{{member.role}}</span></div>
                  </div>
                </div>

              {% endfor %}

            {% endeditable_model %}

          </div>

          {{ 'application.js' | javascript_tag }}

        </div>

      {% endblock %}

      </div>
    </div>
{% endblock %}
